# Kadoa API Configuration
# Copy this file to .env and update with your values

# Your Kadoa API Key
# Get your API key from https://app.kadoa.com/settings/api
KADOA_API_KEY=your-api-key-here

# Kadoa API Endpoint
# Default: https://api.kadoa.com
# For local development: http://localhost:12380
KADOA_PUBLIC_API_URI=https://api.kadoa.com

# Request timeout in milliseconds
KADOA_TIMEOUT=30000

# Enable debug logging (true/false)
DEBUG=false